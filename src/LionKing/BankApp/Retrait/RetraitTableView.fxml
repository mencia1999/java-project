<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" onDragDetected="#handleRetour" prefHeight="500.0" prefWidth="800.0" styleClass="background" stylesheets="@../App/DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LionKing.BankApp.Retrait.RetraitTableViewController">
   <children>
      <ButtonBar layoutX="389.0" layoutY="450.0" prefHeight="40.0" prefWidth="399.0">
        <buttons>
            <Button mnemonicParsing="false" onAction="#handleNewRetrait" text="Ajouter" />
            <Button fx:id="btn_modifier" mnemonicParsing="false" onAction="#handleEditRetrait" text="Modifier" />
            <Button fx:id="btn_supprimer" mnemonicParsing="false" onAction="#handleDeleteRetrait" text="Supprimer" />
        </buttons>
      </ButtonBar>
      <Button layoutX="14.0" layoutY="453.0" mnemonicParsing="false" onAction="#handleRetour" prefHeight="33.0" prefWidth="149.0" text="Retour acceuil" />
      <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="364.0" prefWidth="800.0">
         <items>
            <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="90.0" prefWidth="802.0" styleClass="background">
               <children>
                  <TextField fx:id="recherche" layoutX="298.0" layoutY="40.0" AnchorPane.bottomAnchor="13.0" />
                  <Button layoutX="672.0" layoutY="42.0" mnemonicParsing="false" onAction="#handleRecherche" text="Recherche" />
                  <Button layoutX="14.0" layoutY="42.0" mnemonicParsing="false" onAction="#handleRetourClient" text="Client" />
                  <Label layoutX="148.0" styleClass="label-header" text="RETRAIT" />
                  <ComboBox fx:id="combobox" layoutX="515.0" layoutY="42.0" prefHeight="33.0" prefWidth="150.0" />
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="266.0" prefWidth="600.0">
               <children>
                  <TableView fx:id="RetraitTable" prefHeight="339.0" prefWidth="802.0">
                     <columns>
                        <TableColumn fx:id="NumChequeColumn" prefWidth="75.0" text="N° Chèque" />
                        <TableColumn fx:id="NumComptColumn" minWidth="50.0" prefWidth="127.0" text="N° Compte" />
                        <TableColumn fx:id="MontantRetraitColumn" prefWidth="138.0" text="Montant" />
                        <TableColumn fx:id="DateRetraitColumn" prefWidth="142.0" text="Date" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </AnchorPane>
         </items>
      </SplitPane>
   </children>
</AnchorPane>
